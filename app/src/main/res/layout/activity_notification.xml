<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginFlow.NotificationActivity">

    <View
        android:id="@+id/ellipse_59"
        android:layout_width="match_parent"
        android:layout_height="91dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ellipse_59"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/back_button"
        android:layout_width="21dp"
        android:layout_height="16dp"
        android:layout_marginStart="317dp"
        android:layout_marginLeft="317dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:src="@drawable/top_back_arrow_24"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_59"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ellipse_59" />

    <TextView
        android:id="@+id/notification_headline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginLeft="136dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="135dp"
        android:layout_marginRight="135dp"
        android:layout_marginBottom="42dp"
        android:text="@string/notificationHeadline"
        android:textAppearance="@style/createHeadlineAppearance"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_59"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ellipse_59" />

    <View
        android:id="@+id/expandBox1"
        android:layout_width="327dp"
        android:layout_height="122dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/expandbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ellipse_59" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="85dp"
        android:src="@drawable/time_line"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox1"
        app:layout_constraintEnd_toEndOf="@+id/expandBox1"
        app:layout_constraintHorizontal_bias="0.818"
        app:layout_constraintStart_toStartOf="@+id/time_headline"
        app:layout_constraintTop_toTopOf="@+id/expandBox1"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="272dp"
        android:layout_marginRight="272dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/add_circle"
        android:clickable="true"
        android:onClick="expandTimeBox"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox1"
        app:layout_constraintEnd_toEndOf="@+id/expandBox1"
        app:layout_constraintStart_toStartOf="@+id/expandBox1"
        app:layout_constraintTop_toTopOf="@+id/expandBox1" />
    
    <TextView
        android:id="@+id/time_headline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="@string/time_headline"
        android:layout_marginStart="218dp"
        android:layout_marginLeft="218dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:layout_marginBottom="85dp"
        android:textAppearance="@style/small_headline"
        android:gravity="right|center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox1"
        app:layout_constraintEnd_toEndOf="@+id/expandBox1"
        app:layout_constraintStart_toStartOf="@+id/expandBox1"
        app:layout_constraintTop_toTopOf="@+id/expandBox1"/>

    <TextView
        android:id="@+id/time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19.75dp"
        android:layout_marginLeft="19.75dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="61dp"
        android:text="@string/time_text"
        android:textAppearance="@style/text"
        android:lineSpacingExtra="1sp"
        android:gravity="right|center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox1"
        app:layout_constraintEnd_toEndOf="@+id/expandBox1"
        app:layout_constraintStart_toStartOf="@+id/expandBox1"
        app:layout_constraintTop_toTopOf="@+id/expandBox1"
        />

    <View
        android:id="@+id/expandBox2"
        android:layout_width="327dp"
        android:layout_height="122dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="273dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/expandbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/ellipse_59" />

    <TextView
        android:id="@+id/location_headline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="@string/location_headline"
        android:layout_marginStart="218dp"
        android:layout_marginLeft="218dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:layout_marginBottom="85dp"
        android:textAppearance="@style/small_headline"
        android:gravity="right|center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox2"
        app:layout_constraintEnd_toEndOf="@+id/expandBox2"
        app:layout_constraintStart_toStartOf="@+id/expandBox2"
        app:layout_constraintTop_toTopOf="@+id/expandBox2"/>

    <TextView
        android:id="@+id/location_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19.75dp"
        android:layout_marginLeft="19.75dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="61dp"
        android:text="@string/location_text"
        android:textAppearance="@style/text"
        android:lineSpacingExtra="1sp"
        android:gravity="right|center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox2"
        app:layout_constraintEnd_toEndOf="@+id/expandBox2"
        app:layout_constraintStart_toStartOf="@+id/expandBox2"
        app:layout_constraintTop_toTopOf="@+id/expandBox2"
        />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="291dp"
        android:layout_marginLeft="291dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="85dp"
        android:src="@drawable/map_pin_line"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox2"
        app:layout_constraintEnd_toEndOf="@+id/expandBox2"
        app:layout_constraintStart_toStartOf="@+id/expandBox2"
        app:layout_constraintTop_toTopOf="@+id/expandBox2" />

    <Switch
        android:id="@+id/switch_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="240.75dp"
        android:layout_marginRight="240.75dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/expandBox2"
        app:layout_constraintStart_toStartOf="@+id/expandBox2"
        app:layout_constraintStart_toEndOf="@+id/expandBox2"
        app:layout_constraintTop_toTopOf="@+id/expandBox2"
        android:checked="true" />


</androidx.constraintlayout.widget.ConstraintLayout>